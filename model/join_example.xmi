<?xml version="1.0" encoding="UTF-8"?>
<Workflow:Workflow
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:Contract="https://www.example.org/contract"
    xmlns:Library="http://www.example.org/Library"
    xmlns:Workflow="https://www.example.org/workflow"
    xsi:schemaLocation="https://www.example.org/contract ../metamodel/Contract.ecore http://www.example.org/Library ../metamodel/Library.ecore https://www.example.org/workflow ../metamodel/Workflow.ecore"
    name="join_example">
  <dataprocessing
      xsi:type="Workflow:DataProcessing"
      in="//@dataprocessing.0/@inputPort.0"
      out="//@dataprocessing.0/@outputPort.0"
      name="join(Name with City)">
    <inputPort
        fileName="join_input_dataDictionary.csv"
        name="join(Name with City)_input_dataDictionary"
        out="//@dataprocessing.0/@outputPort.0">
      <datafield
          xsi:type="Workflow:Categorical"
          name="join(Name)_input1_dataField"
          displayName="name">
        <categoricalDef
            href="library_validation.xmi#//@dataprocessingdefinition.11/@inputPort.0/@datafielddefinition.0"/>
      </datafield>
      <datafield
          xsi:type="Workflow:Categorical"
          name="join(City)_input2_dataField"
          displayName="City">
        <categoricalDef
            href="library_validation.xmi#//@dataprocessingdefinition.11/@inputPort.0/@datafielddefinition.0"/>
      </datafield>
      <dataDictionaryDefinition
          href="library_validation.xmi#//@dataprocessingdefinition.11/@inputPort.0"/>
    </inputPort>
    <outputPort
        fileName="join_output_dataDictionary.csv"
        name="join(Name with City)_output_dataDictionary"
        in="//@dataprocessing.0/@inputPort.0">
      <datafield
          xsi:type="Workflow:Categorical"
          name="join(Name with City)_output_dataField"
          displayName="Name with City">
        <categoricalDef
            href="library_validation.xmi#//@dataprocessingdefinition.11/@outputPort.0/@datafielddefinition.0"/>
      </datafield>
      <dataDictionaryDefinition
          href="library_validation.xmi#//@dataprocessingdefinition.11/@outputPort.0"/>
    </outputPort>
    <dataProcessingDefinition
        xsi:type="Library:Transformation"
        href="library_validation.xmi#//@dataprocessingdefinition.11"/>
    <parameter
        xsi:type="Workflow:Join"
        name="join(Name with City)_param_join">
      <operands
          xsi:type="Workflow:OpField"
          name="join(Name with City)_param_op1_field"
          next_operand="//@dataprocessing.0/@parameter.0/@operands.1"
          datafield="//@dataprocessing.0/@inputPort.0/@datafield.0"/>
      <operands
          xsi:type="Workflow:OpFixValue"
          name="join(Name with City)_param_op2_fix_value"
          next_operand="//@dataprocessing.0/@parameter.0/@operands.2"
          value="-"/>
      <operands
          xsi:type="Workflow:OpField"
          name="join(Name with City)_param_op3_field"
          datafield="//@dataprocessing.0/@inputPort.0/@datafield.1"/>
      <joinDef
          href="library_validation.xmi#//@dataprocessingdefinition.11/@parameterdefinition.0"/>
    </parameter>
    <contract
        name="join(Name with City)_specialValue_INVARIANT">
      <contractDefinition
          href="library_validation.xmi#//@dataprocessingdefinition.11/@contractdefinition.0"/>
      <contract
          xsi:type="Contract:Condition"
          type="INVARIANT"
          name="INV_specialValue_condition">
        <in
            xsi:type="Contract:DataField"
            dataField="//@dataprocessing.0/@inputPort.0/@datafield.0"
            name="INV_specialValue_input_datafield_Name">
          <dataFieldDef
              href="library_validation.xmi#//@dataprocessingdefinition.11/@contractdefinition.1/@contractDefinition/@in.0"/>
        </in>
        <in
            xsi:type="Contract:DataField"
            dataField="//@dataprocessing.0/@inputPort.0/@datafield.1"
            name="INV_specialValue_input_datafield_City">
          <dataFieldDef
              href="library_validation.xmi#//@dataprocessingdefinition.11/@contractdefinition.1/@contractDefinition/@in.0"/>
        </in>
        <out
            xsi:type="Contract:DataField"
            dataField="//@dataprocessing.0/@outputPort.0/@datafield.0"
            name="INV_specialValue_output_datafield_Name with City">
          <dataFieldDef
              href="library_validation.xmi#//@dataprocessingdefinition.11/@contractdefinition.1/@contractDefinition/@out.0"/>
        </out>
        <conditionDef
            href="library_validation.xmi#//@dataprocessingdefinition.11/@contractdefinition.0/@contractDefinition"/>
        <if
            belongOp="NOTBELONG"
            then="//@dataprocessing.0/@contract.0/@contract/@then.0"
            name="INV_specialValue_if">
          <ifDefinition
              href="library_validation.xmi#//@dataprocessingdefinition.11/@contractdefinition.0/@contractDefinition/@ifdefinition.0"/>
          <valueCondition
              xsi:type="Contract:SpecialValue">
            <specialValueDef
                href="library_validation.xmi#//@dataprocessingdefinition.11/@contractdefinition.0/@contractDefinition/@ifdefinition.0/@valueCondition.0"/>
          </valueCondition>
        </if>
        <then
            if="//@dataprocessing.0/@contract.0/@contract/@if.0"
            name="INV_specialValue_then"
            belongOp="NOTBELONG">
          <thenDefinition
              href="library_validation.xmi#//@dataprocessingdefinition.11/@contractdefinition.0/@contractDefinition/@thendefinition.0"/>
          <valueResult
              xsi:type="Contract:SpecialValue">
            <specialValueDef
                href="library_validation.xmi#//@dataprocessingdefinition.11/@contractdefinition.0/@contractDefinition/@thendefinition.0/@valueResult"/>
          </valueResult>
        </then>
      </contract>
    </contract>
    <contract
        name="join(Name with City)_join_INVARIANT">
      <contractDefinition
          href="library_validation.xmi#//@dataprocessingdefinition.11/@contractdefinition.0"/>
      <contract
          xsi:type="Contract:Condition"
          type="INVARIANT"
          name="INV_join_condition_Name with City">
        <in
            xsi:type="Contract:DataField"
            dataField="//@dataprocessing.0/@inputPort.0/@datafield.0"
            name="INV_join_input_datafield_Name">
          <dataFieldDef
              href="library_validation.xmi#//@dataprocessingdefinition.11/@contractdefinition.1/@contractDefinition/@in.0"/>
        </in>
        <in
            xsi:type="Contract:DataField"
            dataField="//@dataprocessing.0/@inputPort.0/@datafield.1"
            name="INV_join_input_datafield_City">
          <dataFieldDef
              href="library_validation.xmi#//@dataprocessingdefinition.11/@contractdefinition.1/@contractDefinition/@in.0"/>
        </in>
        <out
            xsi:type="Contract:DataField"
            dataField="//@dataprocessing.0/@outputPort.0/@datafield.0"
            name="INV_join_output_datafield_Name with City">
          <dataFieldDef
              href="library_validation.xmi#//@dataprocessingdefinition.11/@contractdefinition.1/@contractDefinition/@out.0"/>
        </out>
        <conditionDef
            href="library_validation.xmi#//@dataprocessingdefinition.11/@contractdefinition.0/@contractDefinition"/>
        <if
            then="//@dataprocessing.0/@contract.1/@contract/@then.0"
            name="INV_join_if">
          <ifDefinition
              href="library_validation.xmi#//@dataprocessingdefinition.11/@contractdefinition.0/@contractDefinition/@ifdefinition.0"/>
          <valueCondition
              xsi:type="Contract:Interval"
              leftMargin="440.0"
              rightMargin="1600.0"
              closureType="closedClosed">
            <intervalDef
                href="library_validation.xmi#//@dataprocessingdefinition.11/@contractdefinition.1/@contractDefinition/@ifdefinition.0/@valueCondition.0"/>
          </valueCondition>
        </if>
        <then
            if="//@dataprocessing.0/@contract.1/@contract/@if.0"
            name="INV_join_then">
          <thenDefinition
              href="library_validation.xmi#//@dataprocessingdefinition.11/@contractdefinition.0/@contractDefinition/@thendefinition.0"/>
          <valueResult
              xsi:type="Contract:Join">
            <operands
                xsi:type="Contract:OpField"
                name="join(Name with City)_param_op1_field"
                next_operand="//@dataprocessing.0/@contract.1/@contract/@then.0/@valueResult/@operands.1">
              <field
                  dataField="//@dataprocessing.0/@inputPort.0/@datafield.0"/>
            </operands>
            <operands
                xsi:type="Contract:OpFixValue"
                name="join(Name with City)_param_op2_fix_value"
                next_operand="//@dataprocessing.0/@contract.1/@contract/@then.0/@valueResult/@operands.2"
                value="-"/>
            <operands
                xsi:type="Contract:OpField"
                name="join(Name with City)_param_op3_field">
              <field
                  dataField="//@dataprocessing.0/@inputPort.0/@datafield.1"/>
            </operands>
            <joinDef
                href="library_validation.xmi#//@dataprocessingdefinition.11/@contractdefinition.1/@contractDefinition/@thendefinition.0/@valueResult"/>
          </valueResult>
        </then>
      </contract>
    </contract>
  </dataprocessing>
</Workflow:Workflow>
